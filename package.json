{
  "name": "looking-for-group",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "license": "CC0-1.0",
  "workspaces": [
    "client"
  ],
  "scripts": {
    "prepare": "husky",
    "start": "pm2 start ./server/index.js",
    "stop": "pm2 stop ./server/index.js",
    "dev": "concurrently \"npm run build:watch --workspace=client\" \"nodemon server/index.js\"",
    "build:client": "npm run build --workspace=client",
    "test": "cross-env NODE_ENV=test jest",
    "lint:server": "eslint --fix server",
    "lint:server:types": "tsc -p jsconfig.json",
    "lint:client": "npm run lint --workspace=client",
    "format:server": "prettier --write server"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "looking-for-group": "file:",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.10.0",
    "pm2": "^6.0.5",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.28.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.2",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.29",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^30.0.0-beta.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.12.0",
    "esm": "^3.2.25",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.11.0"
  }
}
